public class Main {
    public static void main(String[] args) {
            int num1 = 30;
            int num2 = 20;
            int num3  =10;
                    System.out.println ("subirane: " + (num1 +  num2 + num3));
                    System.out.println ("izvajdane: " + (num1 - num2 - num3));
                    System.out.println ("umnojenie: " + (num1 * num2 * num3));
                }
            }

